actor UseableSwitch 7435 {
	//category OminousFunctions
	speed 5
	radius 5
	Health 8000
	+NOGRAVITY
	+FLOAT
	+SHOOTABLE
	+UseSpecial
	// 0 = Activate
	// 1 = Consume Cabinet Card
	states {
		Spawn:
			TNT1 A 0
		Check1: // Is the player close enough to a switch?
			TNT1 A 1 A_LOOKEX(LOF_NOSOUNDCHECK, 0, 120, 0, 360, "Check2")
			loop
		Check2: // Player is in range. Is the player facing the switch?
			TNT1 A 0 A_JumpIfInTargetLOS("Check3", 40)
			goto Lost
		Check3: // Player is in range and facing the switch. Is the player pressing USE?
			TNT1 A 1 ACS_NamedExecute("SetButtonRange", 0, 1)
			TNT1 A 0 ACS_NamedExecute("ButtonPrompt", 0, Args[4])
			goto Check1
		Lost: // If one of the checks failed, go here
			TNT1 A 1 ACS_NamedExecute("SetButtonRange", 0, 0)
			goto Spawn
		Interact: // All checks completed, player pressed USE
			TNT1 A 1 ACS_NamedExecute("SetButtonRange", 0, 0)
			TNT1 A 1
		Death:
			stop
	}
}

actor UsableSwitchRepeatable : UseableSwitch 7436 {
	//category OminousFunctions
	states {
		Interact: // All checks completed, player pressed USE
			TNT1 A 1 ACS_NamedExecute("SetButtonRange", 0, 0)
			TNT1 A 1
		Death:
			TNT1 A 1 ACS_NamedExecute("SetButtonRange", 0, 0)
			TNT1 A 1 A_SpawnItemEx("UsableSwitchRepeatable", 0, 0, 0, 0, 0, 0, 0, SXF_TRANSFERSPECIAL | SXF_TRANSFERPOINTERS  | SXF_SETTARGET  )
			stop
	}
}

